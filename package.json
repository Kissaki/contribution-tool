{
  "name": "ota-contribution-tool",
  "version": "1.25.0",
  "prettier": "prettier-config-ambanum",
  "scripts": {
    "dev": "NODE_OPTIONS=\"--insecure-http-parser --max-http-header-size=3276\" ts-node --project tsconfig.server.json src/server/index.ts",
    "build": "cp -R src/translations/en src/translations/default && next build --debug && tsc --project tsconfig.server.json && rm -Rf src/translations/default",
    "start": "NODE_ENV=production node dist/src/server/index.js",
    "type-check": "tsc",
    "update-cookie-extension": "src/modules/Scraper/i-dont-care-about-cookies/tools/copy-to-folder.sh",
    "encrypt": "grep -rL \"ANSIBLE\" ./ops/roles/*/*/files/{.env.*.*,.env.*}/.env.secrets| sort -u  | xargs ansible-vault encrypt",
    "decrypt": "grep -rl \"ANSIBLE\" ./ops/roles/*/*/files/{.env.*.*,.env.*}/.env.secrets| sort -u  | xargs ansible-vault decrypt",
    "deploy:vagrant": "ansible-playbook ops/site.yml -i ops/inventories/dev.yml --limit vagrant -vvv",
    "deploy:preproduction": "ansible-playbook ops/site.yml -i ops/inventories/production.yml --limit preproduction -vvv",
    "deploy:production": "ansible-playbook ops/site.yml -i ops/inventories/production.yml  --limit production -vvv",
    "docker:build": "docker build -t ota-contribution-tool . --build-arg ENV_FILE=.env",
    "docker:run": "(docker rm ota-contribution-tool || true) && docker run --rm -p 3000:3000 --name ota-contribution-tool -it ota-contribution-tool",
    "docker:ssh": "docker exec -it ota-contribution-tool /bin/sh",
    "i18n": "yarn i18n:extract && yarn i18n:clean && echo '\n⚠️  You may need to restart your dev server\n'",
    "i18n:extract": "NODE_ENV=development i18next-scanner --config 'src/modules/I18n/i18next-scanner.config.js'",
    "i18n:clean": "find src/translations -size 3c -delete",
    "test": "jest",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@socialgouv/matomo-next": "^1.2.1",
    "@svgr/webpack": "^5.5.0",
    "accept-language": "^3.0.18",
    "autoprefixer": "^10.4.2",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.8",
    "debug": "^4.3.1",
    "dotenv": "^11.0.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.1",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "next": "^12.0.4",
    "next-i18next": "^9.2.0",
    "next-mdx-remote": "^3.0.2",
    "nprogress": "^0.2.0",
    "octokit": "^1.7.1",
    "postcss": "^8.4.8",
    "postcss-animation": "^0.0.12",
    "postcss-calc": "^8.2.4",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^12.1.4",
    "postcss-easings": "^3.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-mixins": "^9.0.2",
    "postcss-nesting": "^10.1.3",
    "puppeteer": "^13.5.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-recaptcha": "^3.5.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "query-string": "^7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.27.1",
    "react-icons": "^4.3.1",
    "react-toastify": "^8.2.0",
    "react-use": "^17.3.2",
    "swr": "^1.2.2",
    "ts-replace-all": "^1.0.0",
    "user-agents": "^1.0.949"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/debug": "^4.1.5",
    "@types/express": "4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "16.7.1",
    "@types/nprogress": "^0.2.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-icons": "^3.0.0",
    "@types/user-agents": "^1.0.2",
    "fast-glob": "^3.2.11",
    "i18next-scanner": "^3.1.0",
    "i18next-scanner-typescript": "^1.0.6",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "prettier-config-ambanum": "^1.0.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "license": "MIT",
  "browserslist": [
    "last 1 version",
    "> 0.3%",
    "IE 10"
  ]
}
